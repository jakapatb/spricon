{
      "name": "spricon",
      "version": "1.0.11",
      "description": "A modern icon bundling powerhouse that transforms your SVG icons into an optimized sprite system with type-safe imports.",
      "author": "Jakkapat Boonroj",
      "license": "MIT",
      "keywords": [
            "svg",
            "sprite",
            "icon",
            "react",
            "typescript"
      ],
      "repository": {
            "type": "git",
            "url": "https://github.com/jakapatb/spricon"
      },
      "scripts": {
            "build": "tsup",
            "start": "node ./dist/index.js",
            "test": "vitest",
            "publish": "npm publish --access public"
      },
      "dependencies": {
            "camelcase": "^8.0.0",
            "svgo": "^3.2.0"
      },
      "peerDependencies": {
            "@svgr/core": ">=8.0.0",
            "@svgr/plugin-jsx": ">=8.0.0",
            "@svgr/plugin-svgo": ">=8.0.0"
      },
      "devDependencies": {
            "@svgr/core": "^8.1.0",
            "@svgr/plugin-jsx": "^8.1.0",
            "@svgr/plugin-prettier": "^8.1.0",
            "@svgr/plugin-svgo": "^8.1.0",
            "@testing-library/jest-dom": "^6.6.3",
            "@testing-library/react": "^16.0.1",
            "@types/node": "^20.11.0",
            "@types/prompts": "^2.4.9",
            "@vitest/ui": "^2.1.5",
            "glob": "^10.3.10",
            "happy-dom": "^15.11.6",
            "prettier": "^3.3.3",
            "react": "^18.3.1",
            "tsup": "^8.0.1",
            "tsx": "^4.19.2",
            "typescript": "^5.3.3",
            "vitest": "^2.1.5"
      },
      "exports": {
            ".": {
                  "types": "./dist/index.d.ts",
                  "import": "./dist/index.mjs",
                  "require": "./dist/index.js"
            }
      },
      "engines": {
            "node": ">=14.0.0"
      },
      "type": "module"
}